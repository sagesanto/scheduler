CREATE TABLE "Candidates" (
	"ID"	INTEGER,
	"Author"	TEXT NOT NULL,
	"DateAdded"	TEXT NOT NULL,
	"DateLastEdited"	TEXT,
	"CandidateName"	TEXT NOT NULL,
	"CandidateType"	TEXT NOT NULL,
	"Updated"	TEXT,
	"StartObservability"	TEXT,
	"EndObservability"	TEXT,
	"RemovedDt"	TEXT,
	"RemovedReason"	TEXT,
	"RejectedReason"	TEXT,
	"RA"	TEXT,
	"Dec"	REAL,
	"dRA"	BLOB,
	"dDec"	REAL,
	"Magnitude"	REAL,
	"RMSE_RA"	REAL,
	"RMSE_Dec"	REAL,
	"nObs"	INTEGER,
	"Score"	INTEGER,
	"ApproachColor"	TEXT,
	"Scheduled"	INTEGER DEFAULT 0,
	"Observed"	INTEGER DEFAULT 0,
	"Processed"	INTEGER DEFAULT 0,
	"Submitted"	INTEGER DEFAULT 0,
	"Notes"	TEXT,
	"CVal1"	BLOB,
	"CVal2"	BLOB,
	"CVal3"	BLOB,
	"CVal4"	BLOB,
	"CVal5"	BLOB,
	"CVal6"	BLOB,
	"CVal7"	BLOB,
	"CVal8"	BLOB,
	"CVal9"	BLOB,
	"CVal10"	BLOB,
	PRIMARY KEY("ID" AUTOINCREMENT)
)